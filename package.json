{
  "name": "opensource-analytics-dashboard",
  "version": "0.0.0",
  "scripts": {
    "prepare": "husky install",
    "ng": "ng",
    "start": "ng serve -o",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint --fix",
    "deploy": "ng deploy --repo=https://github.com/bitovi/opensource-analytics-dashboard.git --base-href=https://bitovi.github.io/opensource-analytics-dashboard/"
  },
  "lint-staged": {
    "src/**/*.{js,ts,scss,md,html,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "ng lint && lint-staged"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.2.3",
    "@angular/cdk": "^14.2.2",
    "@angular/common": "^14.2.3",
    "@angular/compiler": "^14.2.3",
    "@angular/core": "^14.2.3",
    "@angular/forms": "^14.2.3",
    "@angular/material": "^14.2.2",
    "@angular/platform-browser": "^14.2.3",
    "@angular/platform-browser-dynamic": "^14.2.3",
    "@angular/router": "^14.2.3",
    "angular-google-charts": "^2.2.3",
    "date-fns": "^2.28.0",
    "number-to-words": "^1.2.4",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-builders/jest": "^14.0.1",
    "@angular-devkit/build-angular": "^14.2.3",
    "@angular-eslint/builder": "14.0.2",
    "@angular-eslint/eslint-plugin": "14.0.2",
    "@angular-eslint/eslint-plugin-template": "14.0.2",
    "@angular-eslint/schematics": "14.0.2",
    "@angular-eslint/template-parser": "14.0.2",
    "@angular/cli": "~14.2.3",
    "@angular/compiler-cli": "^14.2.3",
    "@bitovi/eslint-config": "^1.3.0",
    "@types/jasmine": "~4.0.0",
    "@types/jest": "^29.0.3",
    "@types/number-to-words": "^1.2.1",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "angular-cli-ghpages": "^1.0.3",
    "eslint": "^8.18.0",
    "husky": "^8.0.1",
    "jasmine-core": "~4.1.0",
    "jest": "^29.0.3",
    "karma-coverage": "~2.2.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "typescript": "~4.7.2"
  }
}
